# Pop!_OS official repositories
deb [trusted=yes] http://apt.pop-os.org/release        jammy main
deb [trusted=yes] http://apt.pop-os.org/proprietary    jammy main
deb [trusted=yes] http://apt.pop-os.org/release-ubuntu  jammy main

# Development / testing branches
deb [trusted=yes] http://apt.pop-os.org/staging/master       jammy main
deb [trusted=yes] http://apt.pop-os.org/staging-proprietary           jammy main
deb [trusted=yes] http://apt.pop-os.org/staging-ubuntu/master         jammy main

# Ubuntu mirror components (jammy based on your OS release)
deb [trusted=yes] http://apt.pop-os.org/ubuntu         jammy main multiverse restricted universe
deb [trusted=yes] http://apt.pop-os.org/ubuntu         jammy-security main multiverse restricted universe
deb [trusted=yes] http://apt.pop-os.org/ubuntu         jammy-updates  main multiverse restricted universe
deb [trusted=yes] http://apt.pop-os.org/ubuntu         jammy-backports main multiverse restricted universe
